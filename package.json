{
  "name": "midilistener.app",
  "version": "0.2.5",
  "description": "MIDI listener app for Mac OS X",
  "author": "AY <a@qx.zone>",
  "license": "GPL-3.0-or-later",
  "git": "https://github.com/alexrster/midi-listener-app",
  "repository": "https://github.com/alexrster/midi-listener-app",
  "main": "src/main.js",
  "scripts": {
    "prestart": "gulp js.libs css.libs assets.libs",
    "prepack": "gulp js.libs css.libs assets.libs",
    "start": "electron-forge start",
    "test": "mocha",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "build": {
    "appId": "zone.qx.midilistener.app",
    "productName": "MIDI listener",
    "mac": {
      "category": "public.app-category.productivity"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "name": "MIDI listener"
          },
          "platforms": [
            "darwin"
          ]
        }
      ]
    }
  },
  "keywords": [
    "midi",
    "mic",
    "tray",
    "electron",
    "mac"
  ],
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-dmg": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "bower": "^1.8.8",
    "electron": "^10.1.3",
    "electron-rebuild": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-filter": "^6.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-load-plugins": "^2.0.5",
    "gulp-order": "^1.2.0",
    "main-bower-files": "^2.13.3",
    "mocha": "^8.1.3",
    "mochawesome": "^6.1.1"
  },
  "dependencies": {
    "easymidi": "^2.0.1",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "events": "^3.2.0",
    "execa": "^4.0.3",
    "fast-xml-parser": "^3.17.4",
    "he": "^1.2.0",
    "moment": "^2.29.1",
    "mqtt": "^4.2.1",
    "util": "^0.12.3"
  }
}
